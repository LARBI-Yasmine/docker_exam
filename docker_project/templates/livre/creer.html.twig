{% block title %}Ajouter un livre
{% endblock %}

{% block body %}
	<h1>
		<center>Ajouter un nouveau livre</center>
	</h1>
	<div class="container">
		<div class="row">
			<div class="col-8">

				{{ form_start(form) }}
				{{ form_widget(form) }}
				<button class="btn btn-primary">Sauvegarder</button>
				<a href="{{ path('app_livre') }}" class="btn btn-primary" style="font-size:12px">Retour</a>
				{{ form_end(form) }}
			</div>
		</div>
	</div>
{% endblock %}
